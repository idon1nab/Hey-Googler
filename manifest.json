{
  "version": "1.0",
  "manifest_version": 3,
  "name": "Hey Googler 개발자용",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "heyGoogler_icon.png",
      "48": "heyGoogler_icon.png",
      "128": "heyGoogler_icon.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "list.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}